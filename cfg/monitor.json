{
  "setup": {
    "refresh": 1,
    "sensorPageTime": 5,
    "timePageTime": 60,
    "timePage": "DATE_h_m_s_1",
    "sensorPageLabel": {
      "fontFamily": "HarmonyOS_Sans_SC_Bold",
      "fontSize": 64,
      "fontColor": "#e0e0e0",
      "y": 20
    },
    "timePageFontSize": 128,
    "displayOnHour": 8,
    "displayOffHour": 23
  },
  "sensorFilter": [
    "^temperature_.*#unit"
  ],
  "mianban": [
    1
  ],
  "diy": [
    {
      "name": "Sensors",
      "sensor": [
        {
          "mode": 1,
          "match": "^temperature_cpu$",
          "name": "CPU Temp",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " \u00b0C"
        },
        {
          "mode": 1,
          "match": "^cpu_usage_percent$",
          "name": "CPU Usage",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %"
        },
        {
          "mode": 1,
          "match": "^mem_usage_percent$",
          "name": "RAM Usage",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %"
        },
        {
          "mode": 1,
          "match": "^temperature_nvme_Composite_(.+)$",
          "name": "NVMe {1}",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " \u00b0C"
        },
        {
          "mode": 1,
          "match": "^storage_ssd\\[(\\d+)\\]_usage_percent$",
          "name": "SSD {1} Usage",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %"
        },
        {
          "mode": 1,
          "match": "^network_.+_address\\d+$",
          "name": "IP Address",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 56,
          "fontColor": -1,
          "textAlign": "center"
        },
        {
          "mode": 1,
          "match": "^network_.+_upload_speed$",
          "name": "Upload",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center"
        },
        {
          "mode": 1,
          "match": "^network_.+_download_speed$",
          "name": "Download",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center"
        }
      ]
    }
  ]
}