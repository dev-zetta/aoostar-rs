{
  "setup": {
    "refresh": 1,
    "sensorPageTime": 5,
    "timePageTime": 60,
    "timePage": "DATE_h_m_s_1",
    "sensorPageLabel": {
      "fontFamily": "HarmonyOS_Sans_SC_Bold",
      "fontSize": 64,
      "fontColor": "#e0e0e0",
      "y": 20
    },
    "timePageFontSize": 128,
    "displayOnHour": 8,
    "displayOffHour": 23
  },
  "sensorFilter": [
    "^temperature_.*#unit"
  ],
  "mianban": [
    1
  ],
  "diy": [
    {
      "name": "Sensors",
      "sensor": [
        {
          "mode": 1,
          "match": "^temperature_cpu$",
          "name": "CPU TEMP",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " \u00b0C",
          "colorThresholds": [[0, "#00ff00"], [65, "#ffff00"], [80, "#ff8800"], [90, "#ff0000"]]
        },
        {
          "mode": 1,
          "match": "^cpu_usage_percent$",
          "name": "CPU USAGE",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %",
          "colorThresholds": [[0, "#00ff00"], [50, "#ffff00"], [75, "#ff8800"], [90, "#ff0000"]]
        },
        {
          "mode": 1,
          "match": "^mem_usage_percent$",
          "name": "RAM USAGE",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %",
          "colorThresholds": [[0, "#00ff00"], [50, "#ffff00"], [75, "#ff8800"], [90, "#ff0000"]]
        },
        {
          "mode": 1,
          "match": "^temperature_nvme_Composite_(.+)$",
          "name": "NVME {1}",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " \u00b0C",
          "colorThresholds": [[0, "#00ff00"], [45, "#ffff00"], [55, "#ff8800"], [65, "#ff0000"]]
        },
        {
          "mode": 1,
          "match": "^storage_ssd\\[(\\d+)\\]_usage_percent$",
          "name": "SSD {1} USAGE",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 120,
          "fontColor": -1,
          "textAlign": "center",
          "decimalDigits": 0,
          "unit": " %"
        },
        {
          "mode": 1,
          "match": "^network_.+_address\\d+$",
          "name": "IP ADDRESS",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 56,
          "fontColor": -1,
          "textAlign": "center"
        },
        {
          "mode": 1,
          "match": "^network_.+_upload_speed$",
          "name": "UPLOAD",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center"
        },
        {
          "mode": 1,
          "match": "^network_.+_download_speed$",
          "name": "DOWNLOAD",
          "x": 480,
          "y": 200,
          "fontFamily": "HarmonyOS_Sans_SC_Bold",
          "fontSize": 80,
          "fontColor": -1,
          "textAlign": "center"
        }
      ]
    }
  ]
}